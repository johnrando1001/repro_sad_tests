{
    "require": {
        "php": "^8"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "symfony/validator": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
