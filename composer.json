{
    "require": {
        "php": "^8"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "roave/security-advisories": "dev-master",
        "symfony/framework-bundle": "^5.3 || ^6.0.2",
        "symfony/validator": "^5.3 || ^6.0.2",
        "nyholm/symfony-bundle-test": "dev-master",
        "fakerphp/faker": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
